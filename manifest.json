{
  "manifest_version": 3,
  "name": "TabStash",
  "version": "1.0",
  "description": "Save and organize your browser tabs with smart session management",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_title": "TabStash"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "homepage_url": "https://github.com/azye/tabstash",
  "author": "AZYE",
  "privacy_policy_url": "https://github.com/azye/tabstash/blob/main/privacy.html"
}